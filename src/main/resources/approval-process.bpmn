<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn" 
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                  id="Definitions_1" 
                  targetNamespace="http://bpmn.io/schema/bpmn" 
                  exporter="Camunda Modeler" 
                  exporterVersion="5.0.0">
  
  <bpmn:process id="approval-process" name="Approval Process" isExecutable="true">
    
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_1" name="Request Created">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Service Task: Log Request -->
    <bpmn:serviceTask id="log-request" name="Log Request" 
                      camunda:delegateExpression="${logRequestDelegate}">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- User Task: Check Request -->
    <bpmn:userTask id="check-request" name="Check Request" 
                   camunda:candidateGroups="checkers">
      <bpmn:documentation>
        Review and approve/reject the request.
        Request ID: ${requestId}
        Title: ${title}
        Maker: ${maker}
      </bpmn:documentation>
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- Exclusive Gateway: Approved? -->
    <bpmn:exclusiveGateway id="Gateway_Approved" name="Approved?">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_Approved</bpmn:outgoing>
      <bpmn:outgoing>Flow_Rejected</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Service Task: Process Approval -->
    <bpmn:serviceTask id="process-approval" name="Process Approval" 
                      camunda:delegateExpression="${processApprovalDelegate}">
      <bpmn:incoming>Flow_Approved</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- Service Task: Process Rejection -->
    <bpmn:serviceTask id="process-rejection" name="Process Rejection" 
                      camunda:delegateExpression="${processRejectionDelegate}">
      <bpmn:incoming>Flow_Rejected</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event: Approved -->
    <bpmn:endEvent id="EndEvent_Approved" name="Request Approved">
      <bpmn:incoming>Flow_4</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- End Event: Rejected -->
    <bpmn:endEvent id="EndEvent_Rejected" name="Request Rejected">
      <bpmn:incoming>Flow_5</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="log-request" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="log-request" targetRef="check-request" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="check-request" targetRef="Gateway_Approved" />
    <bpmn:sequenceFlow id="Flow_Approved" name="Yes" sourceRef="Gateway_Approved" targetRef="process-approval">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Rejected" name="No" sourceRef="Gateway_Approved" targetRef="process-rejection">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="process-approval" targetRef="EndEvent_Approved" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="process-rejection" targetRef="EndEvent_Rejected" />
    
  </bpmn:process>
  
  <!-- BPMN Diagram -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="approval-process">
      
      <!-- Start Event -->
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="152" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="129" y="145" width="82" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Log Request -->
      <bpmndi:BPMNShape id="log-request_di" bpmnElement="log-request">
        <dc:Bounds x="250" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Check Request -->
      <bpmndi:BPMNShape id="check-request_di" bpmnElement="check-request">
        <dc:Bounds x="410" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Gateway -->
      <bpmndi:BPMNShape id="Gateway_Approved_di" bpmnElement="Gateway_Approved" isMarkerVisible="true">
        <dc:Bounds x="565" y="95" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="563" y="71" width="54" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Process Approval -->
      <bpmndi:BPMNShape id="process-approval_di" bpmnElement="process-approval">
        <dc:Bounds x="680" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- Process Rejection -->
      <bpmndi:BPMNShape id="process-rejection_di" bpmnElement="process-rejection">
        <dc:Bounds x="680" y="200" width="100" height="80" />
      </bpmndi:BPMNShape>
      
      <!-- End Event Approved -->
      <bpmndi:BPMNShape id="EndEvent_Approved_di" bpmnElement="EndEvent_Approved">
        <dc:Bounds x="842" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="819" y="145" width="82" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- End Event Rejected -->
      <bpmndi:BPMNShape id="EndEvent_Rejected_di" bpmnElement="EndEvent_Rejected">
        <dc:Bounds x="842" y="222" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="819" y="265" width="82" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="188" y="120" />
        <di:waypoint x="250" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="350" y="120" />
        <di:waypoint x="410" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="510" y="120" />
        <di:waypoint x="565" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Approved_di" bpmnElement="Flow_Approved">
        <di:waypoint x="615" y="120" />
        <di:waypoint x="680" y="120" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="631" y="102" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_Rejected_di" bpmnElement="Flow_Rejected">
        <di:waypoint x="590" y="145" />
        <di:waypoint x="590" y="240" />
        <di:waypoint x="680" y="240" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="601" y="190" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="780" y="120" />
        <di:waypoint x="842" y="120" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="780" y="240" />
        <di:waypoint x="842" y="240" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
  
</bpmn:definitions>
